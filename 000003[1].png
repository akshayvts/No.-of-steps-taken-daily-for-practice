<script type="text/javascript">
$(document).ready(function() {
var auth_saml_url = "https://login.rstudio.cloud";
if (window.location.pathname.indexOf('/register') === 0) {
$('#sso-hide-label').text("No thanks - I'd like to register via RStudio")
}
if (window.location.pathname.indexOf('/login') === 0) {
$('#sso-hide-label').text("No thanks - I'd like to log in via RStudio")
}
function hide_rstudio_elements() {
$('#sso-submit').attr("disabled", false);
$('#login-form-google').hide();
$('#login-form-github').hide();
$('#login-sub-rstudio').hide();
$('#register-sub-rstudio').hide();
$('#additional-info-rstudio').hide();
$('#terms-rstudio').hide();
}
function show_rstudio_elements() {
$('#sso-submit').attr("disabled", true);
$('#login-form-google').show();
$('#login-form-github').show();
$('#login-sub-rstudio').show();
$('#register-sub-rstudio').show();
$('#additional-info-rstudio').show();
$('#terms-rstudio').show();
}
// trigger display of SSO form elements on email domain
$("input[name='email']").change(function () {
$.get(auth_saml_url + '/saml/api/accounts?email=' + $("input[name='email']").val(), function (data) {
if (!jQuery.isEmptyObject(data)) {
hide_rstudio_elements();
$('#sso-login').show();
$('#sso-account').val(data['id']);
$('#sso-submit').html('Log in via ' + data['name']);
if ($('#sso-hide').prop('checked')) {
$('#sso-hide').prop('checked', false);
}
} else {
show_rstudio_elements();
$('#sso-hide').prop('checked', false);
$('#sso-login').hide();
}
});
});
// toggle SSO elements when checkbox is clicked
$('#sso-hide').on('click', function () {
if ($('#sso-hide').prop('checked')) {
show_rstudio_elements();
$('#sso-terms').hide();
} else {
hide_rstudio_elements();
$('#sso-login').show();
$('#sso-terms').show();
}
});
});
</script>

